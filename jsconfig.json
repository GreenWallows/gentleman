{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      "*": [
        "./src/*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@global/*": [
        "./src/global/*"
      ],
      "@editor/*": [
        "./src/editor/*"
      ],
      "@model/*": [
        "./src/model/*"
      ],
      "@projection/*": [
        "./src/projection/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
    }
  }
}