{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "*": [
        "./src/*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@global/*": [
        "./src/global/*"
      ],
      "@editor/*": [
        "./src/editor/*"
      ],
      "@model/*": [
        "./src/model/*"
      ],
      "@projection/*": [
        "./src/projection/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@zenkai": [
        "./node_modules/protolabo-zenkai/dist/zenkai.esm"
      ],
    }
  },
  "include": [
    "src/**/*",
    "node_modules/protolabo-zenkai/**/*"
  ]
}