{
    "mindmap": {
        "nature": "concrete",
        "alias": "Mind map",
        "attribute": {
            "title": {
                "target": "string",
                "projection": [
                    {
                        "type": "field",
                        "view": "text",
                        "placeholder": "Title"
                    }
                ]
            },
            "markers": {
                "target": "set",
                "accept": "marker",
                "min": 1,
                "required": false,
                "projection": [
                    {
                        "type": "field",
                        "view": "list",
                        "orientation": "horizontal",
                        "item": {
                            "after": {
                                "projection": [
                                    {
                                        "layout": {
                                            "type": "wrap",
                                            "style": {
                                                "box": {
                                                    "space": {
                                                        "outer": {
                                                            "right": 3
                                                        }
                                                    }
                                                }
                                            },
                                            "disposition": [
                                                ","
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        "action": {
                            "add": {
                                "projection": [
                                    {
                                        "layout": {
                                            "type": "wrap",
                                            "style": {
                                                "box": {
                                                    "space": {
                                                        "inner": {
                                                            "top": 2,
                                                            "left": 5,
                                                            "bottom": 2,
                                                            "right": 5
                                                        }
                                                    }
                                                }
                                            },
                                            "disposition": [
                                                "Add a marker"
                                            ]
                                        }
                                    }
                                ]
                            },
                            "remove": {
                                "projection": [
                                    {
                                        "layout": {
                                            "type": "wrap",
                                            "style": {
                                                "text": {
                                                    "bold": 800,
                                                    "size": 15,
                                                    "align": "center"
                                                },
                                                "box": {
                                                    "background": {
                                                        "type": "rgb",
                                                        "value": "#e5e5e5"
                                                    },
                                                    "border": {
                                                        "radius": 2
                                                    },
                                                    "space": {
                                                        "inner": {
                                                            "left": 4,
                                                            "right": 1
                                                        }
                                                    }
                                                }
                                            },
                                            "disposition": [
                                                "â¨‰"
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        },
        "component": {
            "central_topic": {
                "alias": "Central topic",
                "attribute": {    
                    "topic": {
                        "target": "central_topic"
                    }
                },
                "projection": [
                    {
                        "layout": {
                            "type": "stack",
                            "orientation": "vertical",
                            "style": {
                                "box": {
                                    "space": {
                                        "outer": {
                                            "top": 12
                                        }
                                    }
                                }
                            },
                            "disposition": [
                                "#topic"
                            ]
                        }
                    }
                ]
            }
        },
        "projection": [
            {
                "element": {
                    "heading": {
                        "layout": {
                            "type": "wrap",
                            "style": {
                                "text": {
                                    "size": 24,
                                    "bold": 800,
                                    "color": {
                                        "type": "rgb",
                                        "value": "#555"
                                    }
                                },
                                "box": {
                                    "space": {
                                        "outer": {
                                            "bottom": 12
                                        }
                                    }
                                }
                            },
                            "disposition": ["Mind Map #title"]
                        }
                    }
                },
                "layout": {
                    "type": "stack",
                    "orientation": "vertical",
                    "disposition": [
                        "$heading",
                        "#markers",
                        "#central_topic:component"
                    ]
                }
            }
        ]
    },
    "marker": {
        "nature": "concrete",
        "alias": "Marker",
        "attribute": {
            "symbol": {
                "target": "string"
            }
        },
        "projection": [
            {
                "tags": [
                    "editor"
                ],
                "layout": {
                    "type": "wrap",
                    "disposition": [
                        "#symbol"
                    ]
                }
            },
            {
                "tags": [
                    "choice"
                ],
                "readonly": true,
                "layout": {
                    "type": "wrap",
                    "disposition": [
                        "Symbol #symbol"
                    ]
                }
            }
        ]
    },
    "topic": {
        "nature": "prototype",
        "attribute": {
            "name": {
                "target": "string"
            },
            "marker": {
                "target": "reference",
                "accept": "marker",
                "required": false
            }
        }
    },
    "central_topic": {
        "prototype": "topic",
        "nature": "concrete",
        "alias": "Central topic",
        "component": {
            "main_topics": {
                "alias": "Main topics",
                "attribute": {
                    "topics": {
                        "target": "set",
                        "accept": "main_topic",
                        "min": 0,
                        "projection": [
                            {
                                "type": "field",
                                "view": "list",
                                "orientation": "horizontal",
                                "item": {
                                    "style": {
                                        "box": {
                                            "space": {
                                                "inner": {
                                                    "top": 10,
                                                    "left": 15,
                                                    "right": 15,
                                                    "bottom": 10
                                                },
                                                "outer": {
                                                    "right": 12,
                                                    "bottom": 12
                                                }
                                            },
                                            "border": {
                                                "size": 4,
                                                "color": "royalblue",
                                                "radius": 10
                                            },
                                            "background": {
                                                "type": "rgb",
                                                "value": "#E1EBF9"
                                            }
                                        }
                                    }
                                },
                                "action": {
                                    "add": {
                                        "projection": [
                                            {
                                                "layout": {
                                                    "type": "stack",
                                                    "orientation": "vertical",
                                                    "style": {
                                                        "box": {
                                                            "space": {
                                                                "inner": 15
                                                            }
                                                        }
                                                    },
                                                    "disposition": [
                                                        {
                                                            "type": "wrap",
                                                            "disposition": [
                                                                {
                                                                    "type": "text",
                                                                    "style": {
                                                                        "text": {
                                                                            "bold": 600
                                                                        }
                                                                    },
                                                                    "disposition": "Main topic"
                                                                },
                                                                {
                                                                    "type": "text",
                                                                    "style": {
                                                                        "text": {
                                                                            "italic": true
                                                                        }
                                                                    },
                                                                    "disposition": "<name>"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "text",
                                                            "style": {
                                                                "text": {
                                                                    "italic": true
                                                                }
                                                            },
                                                            "disposition": "<details?>"
                                                        },
                                                        {
                                                            "type": "text",
                                                            "style": {
                                                                "text": {
                                                                    "bold": 700,
                                                                    "size": 15
                                                                }
                                                            },
                                                            "disposition": "Subtopics"
                                                        },
                                                        {
                                                            "type": "text",
                                                            "style": {
                                                                "text": {
                                                                    "italic": true
                                                                }
                                                            },
                                                            "disposition": "<subtopics>"
                                                        }
                                                    ]
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        ]
                    }
                },
                "projection": [
                    {
                        "layout": {
                            "type": "stack",
                            "orientation": "vertical",
                            "style": {
                                "box": {
                                    "space": {
                                        "inner": {
                                            "top": 12,
                                            "right": 24,
                                            "left": 24,
                                            "bottom": 18
                                        }
                                    }
                                }
                            },
                            "disposition": [
                                "#topics"
                            ]
                        }
                    }
                ]
            }
        },
        "projection": [
            {
                "layout": {
                    "type": "stack",
                    "orientation": "vertical",
                    "style": {
                        "box": {
                            "border": {
                                "size": 6,
                                "color": {
                                    "type": "rgb",
                                    "value": "#5F4B8B"
                                },
                                "radius": 60
                            },
                            "overflow": "hidden"
                        }
                    },
                    "disposition": [
                        {
                            "type": "wrap",
                            "style": {
                                "text": {
                                    "bold": true,
                                    "size": 18,
                                    "align": "center"
                                },
                                "box": {
                                    "space": {
                                        "inner": {
                                            "top": 20,
                                            "right": 15,
                                            "left": 15,
                                            "bottom": 20
                                        }
                                    },
                                    "background": {
                                        "type": "rgb",
                                        "value": "#FFF5EE"
                                    }
                                }
                            },
                            "disposition": [
                                "Central topic #name"
                            ]
                        },
                        "#main_topics:component"
                    ]
                }
            },
            {
                "tags": [
                    "choice"
                ],
                "layout": {
                    "type": "wrap",
                    "disposition": [
                        "Central topic"
                    ]
                }
            }
        ]
    },
    "main_topic": {
        "prototype": "topic",
        "nature": "concrete",
        "alias": "Main topic",
        "component": {
            "sub_topics": {
                "alias": "Sub topics",
                "attribute": {
                    "topics": {
                        "target": "set",
                        "accept": "sub_topic",
                        "min": 0,
                        "projection": [
                            {
                                "type": "field",
                                "view": "list",
                                "orientation": "horizontal",
                                "item": {
                                    "style": {
                                        "box": {
                                            "space": {
                                                "inner": 5,
                                                "outer": {
                                                    "right": 5
                                                }
                                            },
                                            "border": {
                                                "size": 2,
                                                "color": {
                                                    "type": "rgb",
                                                    "value": "#3FBA5E"
                                                },
                                                "radius": 2
                                            },
                                            "background": {
                                                "type": "rgb",
                                                "value": "#D6F9D8"
                                            }
                                        }
                                    }
                                },
                                "action": {
                                    "add": {
                                        "projection": [
                                            {
                                                "layout": {
                                                    "type": "wrap",
                                                    "style": {
                                                        "box": {
                                                            "space": {
                                                                "inner": 5
                                                            }
                                                        }
                                                    },
                                                    "disposition": [
                                                        "Add a subtopic"
                                                    ]
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        ]
                    }
                },
                "projection": [
                    {
                        "layout": {
                            "type": "stack",
                            "orientation": "vertical",
                            "style": {
                                "box": {
                                    "space": {
                                        "outer": {
                                            "top": 12
                                        }
                                    }
                                }
                            },
                            "disposition": [
                                "#topics"
                            ]
                        }
                    }
                ]
            }
        },
        "projection": [
            {
                "element": {
                    "heading": {
                        "layout": {
                            "type": "wrap",
                            "style": {
                                "text": {
                                    "bold": 600
                                }
                            },
                            "disposition": [
                                "Main topic #name (#marker)"
                            ]
                        }
                    }
                },
                "layout": {
                    "type": "stack",
                    "orientation": "vertical",
                    "disposition": [
                        "$heading",
                        "#sub_topics:component"
                    ]
                }
            },
            {
                "tags": [
                    "choice"
                ],
                "layout": {
                    "type": "wrap",
                    "disposition": [
                        "Main topic"
                    ]
                }
            }
        ]
    },
    "sub_topic": {
        "prototype": "topic",
        "nature": "concrete",
        "alias": "Sub topic",
        "component": {
            "sub_topics": {
                "alias": "Sub topics",
                "required": false,
                "attribute": {
                    "topics": {
                        "target": "set",
                        "accept": "sub_topic",
                        "min": 0,
                        "projection": [
                            {
                                "type": "field",
                                "view": "list",
                                "item": {
                                    "style": {
                                        "box": {
                                            "space": {
                                                "inner": 3,
                                                "outer": {
                                                    "bottom": 5
                                                }
                                            },
                                            "border": {
                                                "size": 1,
                                                "color": "gray",
                                                "radius": 2
                                            }
                                        }
                                    }
                                },
                                "action": {
                                    "add": {
                                        "projection": [
                                            {
                                                "layout": {
                                                    "type": "wrap",
                                                    "style": {
                                                        "box": {
                                                            "space": {
                                                                "inner": 5
                                                            }
                                                        }
                                                    },
                                                    "disposition": [
                                                        "Add a subsubtopic"
                                                    ]
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        ]
                    }
                },
                "projection": [
                    {
                        "layout": {
                            "type": "wrap",
                            "disposition": [
                                "#topics"
                            ]
                        }
                    }
                ]
            }
        },
        "projection": [
            {
                "layout": {
                    "type": "stack",
                    "orientation": "vertical",
                    "disposition": [
                        "#name (#marker)",
                        "#sub_topics:component"
                    ]
                }
            },
            {
                "tags": [
                    "choice"
                ],
                "layout": {
                    "type": "wrap",
                    "disposition": [
                        "Subtopic"
                    ]
                }
            }
        ]
    },
    "@root": "mindmap",
    "@editor": {
        "build": false,
        "import": true,
        "export": true
    },
    "@config": {
        "language": "Mind map model",
        "settings": {
            "autosave": true
        }
    }
}