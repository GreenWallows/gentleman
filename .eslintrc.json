{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "off",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": "off",
        "no-useless-escape": "warn"
    },
    "globals": {
        "Autocomplete": false,
        "DataType": false,
        "DOC": false,
        "EventType": false,
        "Exception": false,
        "HELPER": true,
        "HTMLAttribute": false,
        "Key": false,
        "MetaModel": false,
        "ModelAttribute": false,
        "ModelElement": false,
        "ModelType": false,
        "Projection": false,
        "Representation": false,
        "UI": true,
        "UTIL": true
    }
}